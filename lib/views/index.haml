- content_for(:head) do
  %link{:rel => 'stylesheet', :href => '/stylesheets/dashboard.css', :type => 'text/css'}

%table#all_projects
  %tr.headings
    %td.status_icon 
    %td.project_name Project Name
    %td.last_built_at Last Built at
    %td.total_tests 
    
  - projects.each do |project|
    = haml :_project_row, :locals => { :project => project }
